{
  "name": "fastify-backend-typescript-template",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "dev": "node --env-file=.env --import tsx --watch src/initServer.ts",
    "test": "node --import tsx --test src/**/*.test.ts",
    "test:only": "node --import tsx --test-only --test src/**/*.test.ts",
    "benchmark": "node --env-file=.env --import tsx benchmark/benchmark.ts",
    "benchmark:quick": "concurrently -k -s first \"node --env-file=.env --import tsx src/initServer.ts\" \"npx autocannon -c 100 -d 5 -p 10 http://localhost:8000/api/health-check\"",
    "k6:smoke": "k6 run benchmark/k6/smoke-test.js",
    "k6:load": "k6 run benchmark/k6/load-test.js",
    "k6:stress": "k6 run benchmark/k6/stress-test.js",
    "k6:health": "k6 run benchmark/k6/health-check.js",
    "ignore:build:ts": "tsc",
    "ignore:watch:ts": "tsc -w",
    "ignore:test": "npm run build:ts && tsc -p test/tsconfig.json && c8 node --test -r ts-node/register \"test/**/*.ts\"",
    "ignore:start": "npm run build:ts && fastify start -l info dist/initServer.js",
    "ignore:dev2": "npm run build:ts && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"npm:watch:ts\" \"npm:dev:start\"",
    "ignore:dev:start": "fastify start --ignore-watch=.ts$ -w -l info -P dist/initServer.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^6.0.0",
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.1.0",
    "@fastify/etag": "^6.1.0",
    "@fastify/helmet": "^13.0.2",
    "@fastify/redis": "^7.1.0",
    "@fastify/sensible": "^6.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "color-my-json": "^0.0.9",
    "fastify": "^5.0.0",
    "fastify-plugin": "^5.0.0",
    "jsonwebtoken": "^9.0.3",
    "mongoose": "^9.0.1",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@eslint/js": "^9.39.1",
    "@types/autocannon": "^7.12.7",
    "@types/k6": "^1.4.0",
    "@types/node": "^24.10.1",
    "@types/pg": "^8.16.0",
    "autocannon": "^8.0.0",
    "c8": "^10.1.2",
    "concurrently": "^9.0.0",
    "eslint-plugin-perfectionist": "^4.15.1",
    "fastify-cli": "^7.4.1",
    "fastify-tsconfig": "^3.0.0",
    "globals": "^16.5.0",
    "prettier": "^3.7.2",
    "ts-node": "^10.4.0",
    "tsx": "^4.21.0",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.48.0"
  }
}
